{
  "name": "magna-coders-backend",
  "version": "1.0.0",
  "description": "Backend API for magna-coders using Express and Prisma",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:nodemon": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "render-build": "npm install && npm run build && npx prisma generate",
    "render-start": "npx prisma migrate deploy && npm start"
  },
  "dependencies": {
    "@paypal/checkout-server-sdk": "1.0.3",
    "@prisma/client": "6.18.0",
    "@types/cheerio": "^0.22.35",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "axios": "^1.13.5",
    "bcryptjs": "2.4.3",
    "cheerio": "^1.2.0",
    "cloudinary": "2.8.0",
    "cors": "2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "google-auth-library": "^10.6.1",
    "helmet": "7.2.0",
    "jsonwebtoken": "9.0.2",
    "nodemailer": "^7.0.13",
    "pnpm": "^10.28.2",
    "razorpay": "^2.9.6",
    "stripe": "19.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "5.0.1",
    "twilio": "5.10.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.19",
    "@types/express": "4.17.25",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "^20.19.30",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "jest": "^29.7.0",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.14",
    "prettier": "^3.8.1",
    "prisma": "6.18.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
